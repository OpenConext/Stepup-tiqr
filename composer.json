{
  "name": "surfnet/stepup-tiqr",
  "license": "Apache-2.0",
  "description": "Tiqr implemented with GSSP bundle.",
  "type": "project",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Surfnet\\Tiqr\\": "src",
      "Surfnet\\Tiqr\\Dev\\": "dev"
    }
  },
  "require": {
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-zlib": "*",
    "incenteev/composer-parameter-handler": "^2.2",
    "openconext/monitor-bundle": "^4.3.1",
    "paragonie/halite": "^5.1.2",
    "surfnet/stepup-bundle": "^6.0.17",
    "surfnet/stepup-gssp-bundle": "^5.1",
    "surfnet/stepup-saml-bundle": "^6.1",
    "symfony/asset": "^6.4.13",
    "symfony/config": "^6.4.14",
    "symfony/console": "^6.4.15",
    "symfony/dependency-injection": "^6.4.16",
    "symfony/flex": "^2.4.7",
    "symfony/form": "^6.4.13",
    "symfony/monolog-bundle": "^3.10",
    "symfony/runtime": "^6.4.14",
    "symfony/security-bundle": "^6.4.13",
    "symfony/templating": "^6.4.13",
    "symfony/translation": "^6.4.13",
    "symfony/twig-bundle": "^6.4.13",
    "symfony/validator": "^6.4.16",
    "symfony/webpack-encore-bundle": "^2.2",
    "symfony/yaml": "^6.4.13",
    "tiqr/tiqr-server-libphp": "^4.3.1",
    "twig/extra-bundle": "^3.17",
    "twig/twig": "^3.17"
  },
  "require-dev": {
    "behat/mink": "^1.12",
    "behat/mink-browserkit-driver": "^2.2",
    "behat/mink-selenium2-driver": "^1.7",
    "friends-of-behat/symfony-extension": "^2.6",
    "khanamiryan/qrcode-detector-decoder": "^2.0.2",
    "league/csv": "^9.19",
    "malukenho/docheader": "^1.1",
    "overtrue/phplint": ">=9.4.2",
    "phpmd/phpmd": "^2.15",
    "phpstan/phpstan": "^2.0.0",
    "phpstan/phpstan-symfony": "^2.0.0",
    "phpunit/phpunit": "^9.6.22",
    "sebastian/phpcpd": "^6.0.3",
    "slevomat/coding-standard": "^8.15",
    "soyuka/contexts": "^3.3.13",
    "squizlabs/php_codesniffer": "^3.11.2",
    "symfony/browser-kit": "6.4.*",
    "symfony/css-selector": "^6.4.13",
    "symfony/http-client": "6.4.*",
    "symfony/phpunit-bridge": "^6.4.16",
    "symfony/stopwatch": "^6.4.13",
    "symfony/web-profiler-bundle": "^6.4.16"
  },
  "scripts": {
    "check": [
      "@validate",
      "@docheader",
      "@lint",
      "@jscpd",
      "@phpcs",
      "@phpmd",
      "@phpstan",
      "@unit-tests",
      "@jest"
    ],
    "composer-validate": "./ci/qa/validate",
    "docheader": "./ci/qa/docheader",
    "lint": "./ci/qa/lint",
    "phpcs": "./ci/qa/phpcs",
    "phpcbf": "./ci/qa/phpcbf",
    "phpmd": "./ci/qa/phpmd",
    "phpstan": "./ci/qa/phpstan",
    "phpstan-baseline": "./ci/qa/phpstan-update-baseline",
    "unit-tests": "./ci/qa/phpunit",
    "behat": "./ci/qa/behat",
    "jest": "./ci/qa/jest",
    "jscpd": "./ci/qa/jscpd",
    "encore": [
      "yarn encore production"
    ],
    "frontend-install": [
      "yarn install --frozen-lockfile",
      "yarn encore production"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Google\\Task\\Composer::cleanup",
      "@auto-scripts"
    ]
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "endroid/installer": false
    }
  },
  "extra": {
    "runtime": {
      "disable_dotenv": true
    },
    "symfony": {
      "allow-contrib": false,
      "require": "6.4.*"
    },
    "incenteev-parameters": [
      {
        "file": "config/openconext/parameters.yaml"
      }
    ],
    "google/apiclient-services": [
        ""
    ]
  },
  "archive": {
    "exclude": [
      "!/.env",
      "!/.env.local.php",
      "!/var/cache/prod",
      "!/public/build",
      "public/scss",
      "public/typescript",
      "/node_modules",
      "/dev",
      "/translations/sp",
      "/coverage",
      "tests",
      "/build",
      "/travis-ci-apache.conf",
      "/tsconfig.json",
      "/tslint.json",
      "/Vagrantfile",
      "/webpack.config.js",
      "/jest.config.js",
      "/.docheader",
      "/.env.ci",
      "/.env.test",
      "/.env.vm",
      "/*.xml",
      "/*.xml.dist",
      "/config/routes/dev",
      "/config/packages/dev",
      "/config/packages/test",
      "/node_modules",
      "/coverage",
      "/build",
      "/.github",
      "/docs",
      "/src/Features",
      ".gitignore",
      ".travis.*",
      "ci/qa/behat.yml",
      "/vendor/google/apiclient-services/src/*"
    ]
  }
}
